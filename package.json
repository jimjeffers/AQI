{
  "name": "aqi",
  "version": "1.0.0",
  "description": "Easily calculate air quality index values for pollutants.",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/jimjeffers/AQI.git",
  "author": "Jim Jeffers",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.48.0",
    "flow-copy-source": "^1.2.0",
    "jest": "20.x.x"
  },
  "scripts": {
    "prepublish": "npm run flush && npm run babel-prepublish && npm run flow-prepublish",
    "flush": "rm -rf ./lib/**",
    "babel-prepublish": "babel src/ -d lib --ignore *.test.*",
    "flow-prepublish": "flow-copy-source -i **/*.test.js src lib",
    "test": "jest"
  }
}
